{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"strict": true,
		"importHelpers": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,

		"incremental": true,
		"lib": ["DOM", "ESNext"],
		"allowJs": false,
		"checkJs": false,
		"declaration": true,
		"noEmitOnError": true,
		"resolveJsonModule": true
	}
}
